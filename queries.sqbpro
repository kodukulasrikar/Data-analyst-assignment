    sales_channel,
    SUM(amount) AS total_revenue
FROM clinic_sales
WHERE substr(datetime, 1, 4) = '2021'
GROUP BY sales_channel;
</sql><sql name="SQL 2*">SELECT 
    c.uid,
    cu.name,
    SUM(c.amount) AS total_spent
FROM clinic_sales c
JOIN customer cu ON c.uid = cu.uid
WHERE substr(c.datetime, 1, 4) = '2021'
GROUP BY c.uid
ORDER BY total_spent DESC
LIMIT 10;
</sql><sql name="SQL 3*">WITH sales_month AS (
    SELECT 
        cid,
        SUM(amount) AS revenue
    FROM clinic_sales
    WHERE substr(datetime, 1, 7) = '2021-09'
    GROUP BY cid
),
expense_month AS (
    SELECT 
        cid,
        SUM(amount) AS expense
    FROM expenses
    WHERE substr(datetime, 1, 7) = '2021-09'
    GROUP BY cid
),
profit_table AS (
    SELECT
        cl.state,
        cl.cid,
        cl.clinic_name,
        IFNULL(s.revenue, 0) AS revenue,
        IFNULL(e.expense, 0) AS expense,
        (IFNULL(s.revenue, 0) - IFNULL(e.expense, 0)) AS profit
    FROM clinics cl
    LEFT JOIN sales_month s ON cl.cid = s.cid
    LEFT JOIN expense_month e ON cl.cid = e.cid
),
ranked AS (
    SELECT
        *,
        ROW_NUMBER() OVER (
            PARTITION BY state
            ORDER BY profit ASC   
        ) AS rn
    FROM profit_table
)

SELECT *
FROM ranked
WHERE rn = 2         
ORDER BY state;
</sql><sql name="SQL 4"></sql><current_tab id="2"/></tab_sql></sqlb_project>
